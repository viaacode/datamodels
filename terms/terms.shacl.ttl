  sh:name "definition"@en ;
  sh:name "définition"@fr ;
  sh:name "definitie"@nl ;
  sh:severity sh:Violation .

<#EditorialNoteShape>
  a sh:PropertyShape ;
  sh:path skos:editorialNote ;
  sh:nodeKind sh:Literal ;
  sh:datatype rdf:langString ;
