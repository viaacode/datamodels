  sh:name "note éditoriale"@fr ;
  sh:name "redactionele notitie"@nl ;
  sh:severity sh:Violation .

<#ExampleShape>
  a sh:PropertyShape ;
  sh:path skos:example ;
  sh:nodeKind sh:Literal ;
  sh:datatype rdf:langString ;
  sh:name "example"@en ;
  sh:name "exemple"@fr ;
  sh:name "voorbeeld"@nl ;
