@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rel: <http://id.loc.gov/vocabulary/preservation/relationshipSubType/> .
@prefix schema: <https://schema.org/> .
@prefix seq: <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix pav: <http://purl.org/pav/> .

@base <https://data.hetarchief.be/ns/object> .

<>  a owl:Ontology;
    vann:preferredNamespacePrefix "haObj" ;
    vann:preferredNamespaceUri "http://data.hetarchief.be/ns/object/" ;
    dct:author  [
        schema:name   "Milan Valadou" ;
        schema:email  "milan.valadou@meemoo.be"
        
    ] ,
    [
        schema:name   "Miel Vander Sande" ;
        schema:email  "miel.vandersande@meemoo.be"
    ] ,
    [
        schema:name   "Lennert Van de Velde" ;
        schema:email  "lennert.vandevelde@meemoo.be"
    ] ;
    dct:title     "Data model Objects"@en ;
    dct:title     "Modèle de données Objets"@fr ;
    dct:title     "Datamodel Objecten"@nl ;
    dct:created   "2022-05-16" ;
    dct:modified  "2023-01-12" ;
    dct:description """Data model to describe objects."""@en ;
    dct:description """Modèle de données pour décrire des objets."""@fr ;
    dct:description """Datamodel voor het beschrijven van objecten."""@nl ;
    owl:imports <http://www.loc.gov/premis/rdf/v3/> ;
    owl:imports <http://id.loc.gov/vocabulary/preservation/relationshipSubType/> ;
    owl:imports <http://www.w3.org/ns/prov#>;
    pav:version "0.0.1" .

#####################################
#               SHAPES              #
#####################################

<#ObjectShape> a sh:NodeShape ;
  sh:targetClass premis:Object ;
  sh:property [
    sh:path premis:relationship ;
    sh:class premis:Object ;

    sh:name "relationship"@en ;
    sh:name "relatie"@nl ;
    sh:name "relation"@fr ;

    sh:description "Een generieke relatie tussen intellectuele entiteiten, bestanden of representaties."@nl ;
    sh:description "A generic relationship between intellectual entities, files, or representations."@en ;
    sh:description "Une relation générique entre les entités intellectuelles, les fichiers ou les représentations."@fr ;

    sh:message "premis:relationship is not a premis:Object"@en ;
    sh:message "premis:relationship is niet van de klasse premis:Object"@nl ;
    sh:message "premis:relationship n'est pas de la classe premis:Object"@fr ;
    sh:severity sh:Violation ;
  ].

<#IntellectualEntityShape> a sh:NodeShape ;
  sh:targetClass premis:IntellectualEntity ;
  sh:property [
      a sh:PropertyShape ;
      sh:path premis:identifier ;
      sh:class haObj:LocalIdentifier ;

      sh:name "local identifier"@en ;
      sh:name "lokale identifier"@nl ;
      sh:name "identificateur local"@fr ;
      
      sh:description "A local identifier for the intellectual entity."@en ;
      sh:description "Een lokale identifier voor de intellectuele entiteit."@nl ;
      sh:description "Un identificateur local pour l'entité intellectuelle."@fr ;
      
      sh:message "premis:identifier is not a LocalIdentifier"@en ;
      sh:severity sh:Violation ;
  ],
  [
      a sh:PropertyShape ;
      sh:path haObj:primaryIdentifier ;
      sh:class haObj:LocalIdentifier ;

      rdfs:label "primary local identifier"@en ;
      rdfs:label "identificateur local principal"@fr ;
      rdfs:label "primaire lokale identifier"@nl ;
      
      sh:description "The primary local identifier for the intellectual entity."@en ;
      sh:description "De primaire lokale identifier voor de intellectuele entiteit."@nl ;
      sh:description "Un identificateur local principal pour l'entité intellectuelle."@fr ;
      
      sh:message "haObj:primaryIdentifier is not a LocalIdentifier"@en ;
      sh:severity sh:Violation ;
  ],
  [
    sh:path rel:hsp ;
    sh:class premis:IntellectualEntity ;

    sh:name "has Part"@en ;
    sh:name "heeft deel"@nl ;
    sh:name "a partie"@fr ;

    sh:description "Een intellectuele entiteit die deel uitmaakt van deze intellectuele entiteit."@nl ;
    sh:description "An intellectual entity that is part of this intellectual entity."@en ;
    sh:description "Une entité intellectuelle qui fait partie de cette entité intellectuelle."@fr ;

    sh:message "rel:hsp is not a premis:IntellectualEntity"@en ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path rel:isp ;
    sh:class premis:IntellectualEntity ;

    sh:name "is Part Of"@en ;
    sh:name "is deel van"@nl ;
    sh:name "est partie de"@fr ;

    sh:description "De intellectuele entiteit waarvan deze intellectuele entiteit deel uit maakt."@nl ;
    sh:description "The intellectual entity of which this intellectual entity is part of."@en ;
    sh:description "L'entité intellectuelle dont cette entité intellectuelle fait partie de."@fr ;

    sh:message "rel:isp is not a premis:IntellectualEntity"@en ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path rel:isr ;
    sh:class premis:Representation ;

    sh:minCount 1 ;

    sh:name "is Represented By"@en ;
    sh:name "wordt gerepresenteerd door"@nl ;
    sh:name "est représenté par"@fr ;

    sh:description "Een representatie van de intellectuele entiteit."@nl ;
    sh:description "A representation of the intellectual entity."@en ;
    sh:description "Une représentation de l'entité intellectuelle."@fr ;

    sh:message "rel:isr is missing or not a premis:Representation"@en ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:hasMasterCopy ;
    sh:class haObj:DigitalRepresentation ;

    sh:name "has Master"@en ;
    sh:name "heeft master"@nl ;
    sh:name "a maître"@fr ;

    sh:description "De archiefmaster van de intellectuele entiteit."@nl ;
    sh:description "The archive master of the intellectual entity."@en ;
    sh:description "Le maître des archives de l'entité intellectuelle."@fr ;

    sh:message "haObj:hasMasterCopy not a haObj:DigitalRepresentation"@en ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:hasMezzanineCopy ;
    sh:class haObj:DigitalRepresentation ;

    sh:name "has Mezzanine"@en ;
    sh:name "heeft mezzanine"@nl ;
    sh:name "a mezzanine"@fr ;

    sh:description "De mezzanine kopie van de intellectuele entiteit."@nl ;
    sh:description "The mezzanine copy of the intellectual entity."@en ;
    sh:description "La copie mezzanine de l'entité intellectuelle."@fr ;

    sh:message "haObj:hasMezzanineCopy not a haObj:DigitalRepresentation"@en ;
    sh:message "haObj:hasMezzanineCopy is geen haObj:DigitalRepresentation"@nl ;
    sh:message "haObj:hasMezzanineCopy n'est pas un haObj:DigitalRepresentation"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:hasAccessCopy ;
    sh:class haObj:DigitalRepresentation ;

    sh:name "has Access Copy"@en ;
    sh:name "heeft toegangskopie"@nl ;
    sh:name "a copie d'accès"@fr ;

    sh:description "De toegangs- of inkijkkopie van de intellectuele entiteit."@nl ;
    sh:description "The access or browse copy of the intellectual entity."@en ;
    sh:description "L'accès ou la copie de l'entité intellectuelle d'accès."@fr ;

    sh:message "haObj:hasAccessCopy not a haObj:DigitalRepresentation"@en ;
    sh:message "haObj:hasAccessCopy is geen haObj:DigitalRepresentation"@nl ;
    sh:message "haObj:hasAccessCopy n'est pas un haObj:DigitalRepresentation"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:hasIIIFCopy ;
    sh:class haObj:DigitalRepresentation ;

    sh:name "has IIIF Copy"@en ;
    sh:name "heeft IIIF-kopie"@nl ;
    sh:name "a copie de IIIF"@fr ;

    sh:description "De toegangs- of inkijkkopie van de intellectuele entiteit die beschikbaar is via de IIIF Image API."@nl ;
    sh:description "The access or browse copy of the intellectual entity available via IIIF Image API."@en ;
    sh:description "L'accès ou la copie de l'entité intellectuelle d'accès qui est disponible via IIIF Image API."@fr ;

    sh:message "haObj:hasIIIFCopy is not a haObj:DigitalRepresentation"@en ;
    sh:message "haObj:hasIIIFCopy is geen haObj:DigitalRepresentation"@nl ;
    sh:message "haObj:hasIIIFCopy n'est pas un haObj:DigitalRepresentation"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:hasTranscriptionCopy ;
    sh:class haObj:DigitalRepresentation ;

    sh:name "has transcription copy"@en ;
    sh:name "heeft transcriptiekopie"@nl ;
    sh:name "a copie de transcription"@fr ;

    sh:description "Een tekstuele toegangs- of inkijkkopie van de intellectuele entiteit gemaakt door automatische of manuele transcriptie van een andere digitale representatie."@nl ;
    sh:description "A textual access or browse copy of the intellectual entity made by automatic or manual transcription of another digital representation."@en ;
    sh:description "Une copie d'accès textuelle ou de visualisation de l'entité intellectuelle créée par transcription automatique ou manuelle à partir d'une autre représentation numérique."@fr ;

    sh:message "haObj:hasTranscriptionCopy is not a haObj:DigitalRepresentation"@en ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:hasCarrierCopy;
    sh:class haObj:CarrierRepresentation ;

    sh:name "has carrier copy"@en ;
    sh:description "a representation of a physical carrier."@en ;

    sh:message "haObj:hasCarrierCopy is not a haObj:CarrierRepresentation"@en ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path edm:isNextInSequence ;
    sh:class premis:IntellectualEntity ;

    sh:maxCount 1 ;

    sh:name "next intellectual entity in sequence"@en ;
    sh:name "volgende intellectuele entiteit in volgorde"@nl;
    sh:name "l'entité intellectuelle suivante dans l'ordre"@fr;

    sh:description "De intellectuele entiteit die op deze intellectuele entiteit volgt."@nl;
    sh:description "The intellectual entity that follows on this intellectual entity."@en;
    sh:description "L'entité intellectuelle qui suit sur cette entité intellectuelle."@fr;

    sh:message "edm:isNextInSequence has more than one value or is not a premis:File"@en ;
    sh:message "edm:isNextInSequence heeft meer dan één waarde of is geen premis:File"@nl ;
    sh:message "edm:isNextInSequence a plus d'une valeur ou n'est pas un premis:File"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path prov:wasDerivedFrom ;
    sh:class mh:Fragment ;

    sh:minCount 1 ;
    sh:maxCount 1 ;

    sh:name "was derived from"@en ;
    sh:name "werd afgeleid van"@nl;
    sh:name "était dérivé de"@fr;

    sh:description "Het Mediahaven record waarvan deze intellectuele entiteit afgeleid werd."@nl;
    sh:description "The Mediahaven record from which this intellectual entity was derived."@en;
    sh:description "L'enregistrement Mediahaven à partir duquel cette entité intellectuelle est dérivée."@fr;

    sh:message "the object of prov:wasDerivedFrom is no mh:Fragment, is absent or occurs more than once"@en ;
    sh:severity sh:Violation ;
  ].

<#PhysicalCarrierShape> a sh:NodeShape ;
  sh:targetClass haObj:PhysicalCarrier ;
  sh:property [
    sh:path premis:medium ;
    sh:class premis:StorageMedium ;

    sh:minCount 1 ;
    sh:maxCount 1 ;

    sh:name "storage medium"@en ;
    sh:name "opslagmedium"@nl ;
    sh:name "milieu de stockage"@fr ;

    sh:description "The type of storage medium of the physical carrier."@en ;
    sh:description "Het type opslagmedium van de fysieke drager."@nl ;
    sh:description "Le type de milieu de stockage du porteur physique."@fr ;

    sh:message "premis:medium has more than one value, no value or not a premis:StorageMedium"@en ;
    sh:message "premis:medium heeft meer dan één of geen waarde of is geen premis:StorageMedium"@nl ;
    sh:message "premis:medium a plus d'une valeur, pas de valeur ou n'est pas une premis:StorageMedium"@fr ;
    sh:severity sh:Violation ;
  ] .

<#RepresentationShape> a sh:NodeShape ;
  sh:targetClass premis:Representation ;
  sh:property    [
    sh:path rel:rep ;
    sh:class premis:IntellectualEntity ;

    sh:minCount 1 ;
    sh:maxCount 1 ;

    sh:name "represents"@en ;
    sh:name "representeert"@nl ;
    sh:name "représente"@fr ;

    sh:description "De intellectuele entiteit die deze digitale representatie representeert."@nl ;
    sh:description "The intellectual entity that represents this digital representation."@en ;
    sh:description "L'entité intellectuelle qui représente cette représentation digital."@fr ;

    sh:message "rel:rep is missing or not a premis:IntellectualEntity"@en ;
    sh:message "rel:rep ontbreekt of is geen premis:IntellectualEntity"@nl ;
    sh:message "rel:rep manque ou n'est pas une premis:IntellectualEntity"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path prov:wasDerivedFrom ;
    sh:class mh:Fragment ;

    sh:minCount 1 ;
    sh:maxCount 1 ;

    sh:name "was derived from"@en ;
    sh:name "werd afgeleid van"@nl;
    sh:name "était dérivé de"@fr;

    sh:description "Het Mediahaven fragment waarvan deze digitale representatie afgeleid werd."@nl;
    sh:description "The Mediahaven fragment from which this digital representation was derived."@en;
    sh:description "Le fragment Mediahaven à partir duquel cette représentation digitale est dérivée."@fr;

    sh:message "the object of prov:wasDerivedFrom is no mh:Record, is absent or occurs more than once"@en ;
    sh:severity sh:Violation ;
  ].

<#CarrierRepresentationShape> a sh:NodeShape ;
  sh:targetClass haObj:CarrierRepresentation ;
  sh:property [
    sh:path premis:storedAt ;
    sh:class haObj:PhysicalCarrier ;

    sh:minCount 1 ;
    sh:maxCount 1 ;

    sh:name "stored at"@en ;
    sh:name "opgeslagen op"@nl ;
    sh:name "stocké à"@fr ;

    sh:description "De fysieke drager waarop de representatie is opgeslagen."@nl ;
    sh:description "The pysical carrier where the representation is stored."@en ;
    sh:description "Le porteur physique où la représentation est stocké."@fr ;

    sh:message "premis:storedAt has more than one value, no values or not a haObj:PhysicalCarrier"@en ;
    sh:message "premis:storedAt heeft meer dan één of geen waarde of is geen haObj:PhysicalCarrier"@nl ;
    sh:message "premis:storedAt a plus d'une valeur, pas de valeur ou n'est pas une haObj:PhysicalCarrier"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:isCarrierCopyOf ;
    sh:class premis:IntellectualEntity ;

    sh:maxCount 1 ;

    sh:name "is carrier copy of"@en ;
    sh:description "The intellectual entity of which this digital carrier is the copy."@en ;

    sh:message "haObj:isCarrierCopyOf has more than one value or not a premis:IntellectualEntity"@en ;
    sh:severity sh:Violation ;
  ].

<#DigitalRepresentationShape> a sh:NodeShape ;
  sh:targetClass haObj:DigitalRepresentation ;
  sh:property  [
    sh:path haObj:isMasterCopyOf ;
    sh:class premis:IntellectualEntity ;

    sh:maxCount 1 ;

    sh:name "is Master Of"@en ;
    sh:name "is master van"@nl ;
    sh:name "est maître de"@fr ;

    sh:description "De intellectuele entiteit waarvan deze representatie de archiefmaster is."@nl ;
    sh:description "The intellectual entity of which this representation is the archive master."@en ;
    sh:description "L'entité intellectuelle dont cette représentation est le maître des archives."@fr ;

    sh:message "haObj:isMasterCopyOf has more than one value or not a premis:IntellectualEntity"@en ;
    sh:message "haObj:isMasterCopyOf heeft meer dan één waarde of is geen premis:IntellectualEntity"@nl ;
    sh:message "haObj:isMasterCopyOf a plus d'une valeur ou n'est pas une premis:IntellectualEntity"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:isMezzanineCopyOf ;
    sh:class premis:IntellectualEntity ;

    sh:maxCount 1 ;

    sh:name "is Mezzanine Of"@en ;
    sh:name "is mezzanine van"@nl ;
    sh:name "est mezzanine de"@fr ;

    sh:description "De intellectuele entiteit waarvan deze digitale representatie de mezzanine kopie is."@nl ;
    sh:description "The intellectual entity of which this digital representation is the mezzanine copy."@en ;
    sh:description "L'entité intellectuelle dont cette représentation est la copie de mezzanine."@fr ;

    sh:message "haObj:isMezzanineCopyOf has more than one value or not a premis:IntellectualEntity"@en ;
    sh:message "haObj:isMezzanineCopyOf heeft meer dan één waarde of is geen premis:IntellectualEntity"@nl ;
    sh:message "haObj:isMezzanineCopyOf a plus d'une valeur ou n'est pas une premis:IntellectualEntity"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:isAccessCopyOf ;
    sh:class premis:IntellectualEntity ;

    sh:maxCount 1 ; 

    sh:name "is Access Copy Of"@en ;
    sh:name "is toegangskopie van"@nl ;
    sh:name "est copie d'accès de"@fr ;

    sh:description "De intellectuele entiteit waarvan deze digitale representatie de toegangs- of inkijkkopie is."@nl ;
    sh:description "The intellectual entity of which this digital representation is the access or browse copy."@en ;
    sh:description "L'entité intellectuelle dont cette représentation digital est l'accès ou la copie de vue."@fr ;

    sh:message "haObj:isAccessCopyOf has more than one value or not a premis:IntellectualEntity"@en ;
    sh:message "haObj:isAccessCopyOf heeft meer dan één waarde of is geen premis:IntellectualEntity"@nl ;
    sh:message "haObj:isAccessCopyOf a plus d'une valeur ou n'est pas une premis:IntellectualEntity"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:isIIIFCopyOf ;
    sh:class premis:IntellectualEntity ;

    sh:maxCount 1 ; 

    sh:name "is IIIF copy of"@en ;
    sh:name "is IIIF-kopie van"@nl ;
    sh:name "est copie IIIF de"@fr ;

    sh:description "De intellectuele entiteit waarvan deze digitale representatie de IIIF toegangskopie is."@nl ;
    sh:description "The intellectual entity of which this digital representation is the IIIF access copy."@en ;
    sh:description "L'entité intellectuelle dont cette représentation digital est l'accès IIIF."@fr ;

    sh:message "haObj:isIIIFCopyOf has more than one value or is not a premis:IntellectualEntity"@en ;
    sh:message "haObj:isIIIFCopyOf heeft meer dan één waarde of is geen premis:IntellectualEntity"@nl ;
    sh:message "haObj:isIIIFCopyOf a plus d'une valeur ou n'est pas une premis:IntellectualEntity"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path haObj:isTranscriptionCopyOf ;
    sh:class premis:IntellectualEntity ;

    sh:maxCount 1 ; 

    sh:name "is transcription copy of"@en ;
    sh:name "is transcriptiekopie van"@nl ;
    sh:name "est copie transcription de"@fr ;

    sh:description "De intellectuele entiteit waarvan deze digitale representatie een transcriptietoegangskopie is."@nl ;
    sh:description "The intellectual entity of which this digital representation is a transcription access copy."@en ;
    sh:description "L'entité intellectuelle dont cette représentation digitale est la copie d'accès transcription."@fr ;

    sh:message "haObj:isTranscriptionCopyOf has more than one value or is not a premis:IntellectualEntity"@en ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path rel:inc ;
    sh:class premis:File ;

    sh:minCount 1 ;

    sh:name "includes"@en ;
    sh:name "bevat"@nl ;
    sh:name "comprend"@fr ;

    sh:description "Een bestand dat deel uitmaakt van deze digitale representatie."@nl ;
    sh:description "A file that is part of this digital representation."@en ;
    sh:description "Un fichier qui fait partie de cette représentation digital."@fr ;

    sh:message "rel:inc is missing or not a premis:File"@en ;
    sh:message "rel:inc ontbreekt of is geen premis:File"@nl ;
    sh:message "rel:inc manque ou n'est pas une premis:File"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path rel:hsr ;
    sh:class premis:File ;

    sh:minCount 1 ;

    sh:name "has Root"@en ;
    sh:name "heeft root"@nl ;
    sh:name "a racine"@fr ;

    sh:description "Het bestand dat eerst verwerkt moet worden."@nl ;
    sh:description "The file that must be processed first."@en ;
    sh:description "Le fichier qui doit être traité en premier."@fr ;

    sh:message "rel:hsr is missing or not a premis:File"@en ;
    sh:message "rel:hsr ontbreekt of is geen premis:File"@nl ;
    sh:message "rel:hsr manque ou n'est pas une premis:File"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path rel:hss ;
    sh:class premis:Representation ;
    # sh:or (
    #   [ sh:class haObj:CarrierRepresentation ] 
    #   [ sh:class haObj:DigitalRepresentation ] 
    # );

    sh:name "has Source"@en ;
    sh:name "heeft bron"@nl ;
    sh:name "a source"@fr ;

    sh:description "De fysieke drager waarvan deze digitale representatie afkomstig is."@nl ;
    sh:description "The physical carrier from which this digital representation originates from."@en ;
    sh:description "Le porteur physique dont vient cette représentation numérique."@fr ;

    sh:message "rel:hss is not a premis:Representation"@en ;
    sh:message "rel:hss is geen premis:Representation"@nl ;
    sh:message "rel:hss n'est pas un premis:Representation"@fr ;
    sh:severity sh:Violation ;
  ],
  [
    sh:path edm:isNextInSequence ;
    sh:class haObj:DigitalRepresentation ;

    sh:maxCount 1 ;

    sh:name "next digital representation in sequence"@en ;
    sh:name "volgende digitale representatie in volgorde"@nl;
    sh:name "la représentation numérique suivante dans l'ordre"@fr;

    sh:description "De digitale representatie dat op deze digitale representatie volgt."@nl;
    sh:description "The digital representation that follows on this digital representation."@en;
    sh:description "La représentation numérique qui suit sur cette représentation numérique."@fr;

    sh:message "edm:isNextInSequence has more than one value or is not a haObj:DigitalRepresentation"@en ;
    sh:message "edm:isNextInSequence heeft meer dan één waarde of is geen haObj:DigitalRepresentation"@nl ;
    sh:message "edm:isNextInSequence a plus d'une valeur ou n'est pas un haObj:DigitalRepresentation"@fr ;
    sh:severity sh:Violation ;
  ].

<#FragmentRepresentationShape> a sh:NodeShape ;
  sh:targetClass haObj:FragmentRepresentation ;
  sh:property  [
        a sh:PropertyShape ;
        sh:path schema:startTime ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:time ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:name "startTime"@en ;
        sh:name "starttijd"@nl ;
        sh:name "heure de départ"@fr ;
        sh:description "Het tijdstip waarop het fragment start."@nl;
        sh:description "The time at which the fragment starts."@en;
