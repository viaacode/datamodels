    haOndThema:kunst.

haOndThema:taal a skos:Concept; 
    skos:inScheme o:thema ;
    skos:topConceptOf o:thema;
    skos:narrower haOndThema:nederlandse-taal, 
        haOndThema:moderne-talen, 
        haOndThema:klassieke-talen, 
        haOndThema:literatuur, 
        haOndThema:media-en-communicatie;
    skos:prefLabel "taal"@nl.

