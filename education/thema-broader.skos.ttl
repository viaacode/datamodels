haOndCol:category a skos:Collection;
    skos:member haOndThema:taal,
    haOndThema:sport-en-gezondheid,
    haOndThema:maatschappij-en-welzijn,
    haOndThema:leren-en-onderzoeken,
    haOndThema:levensbeschouwing,
