@prefix haNePa: <https://data.hetarchief.be/ns/newspaper/> .
@prefix haObj: <https://data.hetarchief.be/ns/object/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Classes

haNePa:NewspaperEditionType   a   rdfs:Class ;
    rdfs:label "van het type Newspaper Edition"@nl ;
    rdfs:label "of type Newspaper Edition"@en ;
    rdfs:label "du genre Newspaper Edition"@fr ;
    skos:definition """NewspaperEditionType is van toepassing op media in meemoo's archief dat oorspronkelijk afkomstig is van een analoge drager van het type krant, en de kranteneditie als geheel voorstelt."""@nl ;
    skos:definition """NewspaperEditionType applies to media in meemoo's archive that is originally derived from an analog carrier of type newspaper, and represents the newspaper edition as a whole."""@en ;
    skos:definition """NewspaperEditionType s'applique aux médias dans les archives de meemoo qui proviennent à l'origine d'un support analogue du type journal, et représentent l'édition du journal dans son ensemble."""@fr ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/newspaper/> .

haNePa:NewspaperPageType   a   rdfs:Class ;
    rdfs:label "van het type Newspaper Page"@nl ;
    rdfs:label "of type Newspaper Page"@en ;
    rdfs:label "du type Newspaper Page"@fr ;
    skos:definition """NewspaperPageType is van toepassing op media in meemoo's archief dat oorspronkelijk afkomstig is van een analoge drager van het type krant, en een specifieke krantenpagina voorstelt."""@nl ;
    skos:definition """NewspaperPageType applies to media in meemoo's archive that is originally derived from an analog carrier of type newspaper, and represents a specific newspaper page."""@en ;
    skos:definition """NewspaperPageType s'applique aux médias des archives de meemoo qui proviennent à l'origine d'un support analogue du type journal, et représentent une page de journal spécifique."""@fr ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/newspaper/> .

# Properties

haNePa:issueType a rdf:Property ;
    rdfs:domain haNePa:NewspaperEditionType ;
    rdfs:range skos:Concept ;
    rdfs:label "uitgavetype"@nl ;
    rdfs:label "publication type"@en ;
    rdfs:label "type de publication"@fr ;
    skos:definition "Het specifieke uitgavetype van een kranteneditie (bv. ochtend-, avond-, weekendkranteneditie etc.)."@nl;
    skos:definition "The specific publication type of a newspaper edition (e.g. morning, evening, weekend newspaper edition, etc.)."@en;
    skos:definition "Le type de publication spécifique d'une édition de journal (par exemple, édition du journal du matin, du soir, du week-end, etc.)."@fr;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/newspaper/> .

haNePa:issuanceFrequency a rdf:Property ;
    rdfs:domain haNePa:NewspaperEditionType ;
    rdfs:range skos:Concept ;
    rdfs:label "uitgavefrequentie"@nl ;
    rdfs:label "frequency of issuance"@en ;
    rdfs:label "fréquence d'émission"@fr ;
    skos:definition "De frequentie volgens welke een krant wordt uitgegeven."@nl;
    skos:definition "The frequency at which a newspaper is published."@en;
    skos:definition "La fréquence à laquelle un journal est publié."@fr;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/newspaper/> .

haNePa:numberOfPages a rdf:Property ;
    rdfs:domain haObj:CarrierRepresentation ;
    rdfs:range rdfs:Literal ;
    skos:related schema:numberOfPages ;
    rdfs:label "aantal pagina's"@nl ;
    rdfs:label "number of pages"@en ;
    rdfs:label "nombre de pages"@fr ;
    skos:definition "Het aantal pagina's waaruit een kranteneditie bestaat."@nl;
    skos:definition "The number of pages that makes up a newspaper edition."@en;
    skos:definition "Le nombre de pages qui composent une édition de journal."@fr;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/newspaper/> .

haNePa:pageNumber a rdf:Property ;
    rdfs:domain haNePa:NewspaperPageType ;
    rdfs:range rdfs:Literal ;
    rdfs:label "paginanummer"@nl ;
    rdfs:label "page number"@en ;
    rdfs:label "numéro de page"@fr ;
    skos:definition "Het volgnummer van een krantenpagina binnen zijn editie."@nl;
    skos:definition "The serial number of a newspaper page within its edition."@en;
    skos:definition "Le numéro de série d'une page de journal dans son édition."@fr;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/newspaper/> .