    [
        a sh:PropertyShape ;
        sh:path schema:alternateName ;
        sh:nodeKind sh:Literal ;
        sh:datatype rdf:langString ;
        sh:name "alternate name"@en ;
        sh:name "alternatieve naam"@nl ;
        sh:name "nom alternatif"@fr ;
        sh:severity sh:Violation ;
        sh:message "schema:alternateName is not of type string"@en ;
        sh:message "schema:alternateName is niet van het type string"@nl ;
        sh:message "schema:alternateName n'est pas de type string"@fr ;
    ],
    [
        a sh:PropertyShape ;
        sh:path schema:isPartOf ;
        sh:or (
            [ sh:class schema:CreativeWork ]
            [ sh:class schema:BroadcastEvent ]
        );
        sh:name "is part of"@en ;
        sh:name "is deel van"@nl ;
        sh:name "est partie de"@fr ;
        sh:severity sh:Violation ;
        sh:message "schema:isPartOf is not of class schema:ArchiveComponent, schema:CreativeWorkSeries, schema:Episode, schema:Collection or schema:BroadcastEvent."@en ;
        sh:message "schema:isPartOf is niet van de klasse schema:ArchiveComponent, schema:CreativeWorkSeries, schema:Episode, schema:Collection of schema:BroadcastEvent."@nl ;
        sh:message "schema:isPartOf n'est pas de la classe schema:ArchiveComponent, schema:CreativeWorkSeries, schema:Episode, schema:Collection ou schema:BroadcastEvent."@fr ;
    ],
    [
        a sh:PropertyShape ;
        sh:path schema:dateCreated ;
        sh:nodeKind sh:Literal ;
        sh:or ( 
            [ sh:datatype edtf:EDTF-level0 ]
            [ sh:datatype edtf:EDTF-level1 ]
            [ sh:datatype edtf:EDTF-level2 ] 
