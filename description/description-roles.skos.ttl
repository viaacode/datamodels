    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "client"@en,
        "opdrachtgever"@nl .

<https://data.hetarchief.be/id/role/co-producer> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/contributor> ;
