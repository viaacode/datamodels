    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "director"@en,
        "regisseur"@nl .

<https://data.hetarchief.be/id/role/distributor> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/publisher> ;
