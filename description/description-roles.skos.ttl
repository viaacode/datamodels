        "bijdrager"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/role> .

<https://data.hetarchief.be/id/role/costume-designer> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/contributor> ;
