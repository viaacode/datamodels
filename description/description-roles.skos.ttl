    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "maker"@en,
        "maker"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/role> .

<https://data.hetarchief.be/id/role/makeup-artist> a skos:Concept ;
