    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "makeup artist"@en,
        "make-up"@nl .

<https://data.hetarchief.be/id/role/manufacturer> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/contributor> ;
    skos:definition "Company that produced the depicted good or product."@en,
        "Bedrijf dat het afgebeelde goed of product geproduceerd heeft."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "manufacturer"@en,
        "fabrikant"@nl .

<https://data.hetarchief.be/id/role/musician> a skos:Concept ;
