    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "announcer"@en,
        "omroeper"@nl .

<https://data.hetarchief.be/id/role/architect> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/maker> ;
    skos:definition "The architect of the depicted building."@en,
        "De architect van het afgebeelde gebouw."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "architect"@en,
        "architect"@nl .

<https://data.hetarchief.be/id/role/archivist> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/maker> ;
    skos:definition "The archivist is a person, organization, company or government that created the archive and after whom the archive is named."@en,
        "De archivist is een persoon, organisatie, bedrijf of overheid die het archief heeft gevormd en waar het archief bijgevolg naar is vernoemd."@nl ;
    skos:example "het archief van John Hope Franklin, het archief van Heemkring Heidebloemke, het archief van de familie Kobayashi, het archief van Agfa-Gevaert NV, het archief van KV Kortrijk, enzovoort."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "archivist"@en,
        "archiefvormer"@nl .

<https://data.hetarchief.be/id/role/arranger> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/contributor> ;
    skos:definition "The arranger is the person who adapts an existing piece of music or composition for a different instrumentation, a specific style or a specific occasion. The result is called 'arrangement'."@en,
        "De arrangeur is de persoon die een bestaand stuk muziek of compositie bewerkt voor een andere bezetting, een bepaalde stijl of een bepaalde gelegenheid. Het resultaat heet 'arrangement'."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "arranger"@en,
        "arrangeur"@nl .

<https://data.hetarchief.be/id/role/artist> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/maker> ;
    skos:definition "The artist is the author of the work of art."@en,
        "De kunstenaar is de auteur van het kunstwerk."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "artist"@en,
        "kunstenaar"@nl .

<https://data.hetarchief.be/id/role/artistic-director> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/contributor> ;
