    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "co-producer"@en,
        "coproducent"@nl .

<https://data.hetarchief.be/id/role/commentator> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/contributor> ;
    skos:definition "The commentator is someone who provides commentary on current events in the media."@en,
        "De commentator is iemand die actuele gebeurtenissen van commentaar voorziet in de media."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "commentator"@en,
        "commentator"@nl .

<https://data.hetarchief.be/id/role/composer> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/maker> ;
    skos:definition "The composer is the person who composed the music."@en,
        "De componist is de persoon die de muziek heeft gecomponeerd."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "composer"@en,
        "componist"@nl .

<https://data.hetarchief.be/id/role/conductor> a skos:Concept ;
    skos:altLabel "dirigente"@nl ;
