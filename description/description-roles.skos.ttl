    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "librarian"@en,
        "archivaris"@nl .

<https://data.hetarchief.be/id/role/lighting> a skos:Concept ;
    skos:broader <https://data.hetarchief.be/id/role/contributor> ;
    skos:definition "The person responsible for lighting then filming or photographing."@en,
        "De persoon verantwoordelijk voor de belichting dan de film- of foto-opname."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/role> ;
    skos:prefLabel "lighting"@en,
        "belichting"@nl .

<https://data.hetarchief.be/id/role/maker> a skos:Concept ;
