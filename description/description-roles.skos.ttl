    skos:prefLabel "digitization partner"@en,
        "digitaliseringspartner"@nl .

<https://data.hetarchief.be/id/role/director> a skos:Concept ;
    skos:altLabel "regisseuse"@nl ;
    skos:broader <https://data.hetarchief.be/id/role/maker> ;
