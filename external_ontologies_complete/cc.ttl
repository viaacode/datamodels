@prefix cc: <http://creativecommons.org/ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xhv: <http://www.w3.org/1999/xhtml/vocab#> .
@prefix dc: <http://purl.org/dc/terms/> .

cc:DerivativeWorks
  a cc:Permission ;
  rdfs:comment "distribution of derivative works"@en-US ;
  rdfs:label "Derivative Works"@en-US .

cc:Distribution
  a cc:Permission ;
  rdfs:comment "distribution, public display, and publicly performance"@en-US ;
  rdfs:label "Distribution"@en-US .

cc:Reproduction
  a cc:Permission ;
  rdfs:comment "making multiple copies"@en-US ;
  rdfs:label "Reproduction"@en-US .

cc:Sharing
  a cc:Permission ;
  rdfs:comment "permits commercial derivatives, but only non-commercial distribution"@en-US ;
  rdfs:label "Sharing"@en-US .

cc:CommercialUse
  a cc:Prohibition ;
  rdfs:comment "exercising rights for commercial purposes"@en-US ;
  rdfs:label "Commercial Use"@en-US .

cc:HighIncomeNationUse
  a cc:Prohibition ;
  rdfs:comment "use in a non-developing country"@en-US ;
  rdfs:label "High Income Nation Use"@en-US .

cc:Attribution
  a cc:Requirement ;
  rdfs:comment "credit be given to copyright holder and/or author"@en-US ;
  rdfs:label "Attribution"@en-US .

cc:Copyleft
  a cc:Requirement ;
  rdfs:comment "derivative and combined works must be licensed under specified terms, similar to those on the original work"@en-US ;
  rdfs:label "Copyleft"@en-US .

cc:LesserCopyleft
  a cc:Requirement ;
  rdfs:comment "derivative works must be licensed under specified terms, with at least the same conditions as the original work; combinations with the work may be licensed under different terms"@en-US ;
  rdfs:label "Lesser Copyleft"@en-US .

cc:Notice
  a cc:Requirement ;
  rdfs:comment "copyright and license notices be kept intact"@en-US ;
  rdfs:label "Notice"@en-US .

cc:ShareAlike
  a cc:Requirement ;
  rdfs:comment "derivative works be licensed under the same terms or compatible terms as the original work"@en-US ;
  rdfs:label "Share Alike"@en-US .

cc:SourceCode
  a cc:Requirement ;
  rdfs:comment "source code (the preferred form for making modifications) must be provided when exercising some rights granted by the license."@en-US ;
  rdfs:label "Source Code"@en-US .

<http://web.resource.org/cc/Attribution> owl:equivalentClass cc:Attribution .
<http://web.resource.org/cc/CommercialUse> owl:equivalentClass cc:CommercialUse .
<http://web.resource.org/cc/DerivativeWorks> owl:equivalentClass cc:DerivativeWorks .
<http://web.resource.org/cc/Distribution> owl:equivalentClass cc:Distribution .
<http://web.resource.org/cc/License> owl:equivalentClass cc:License .
<http://web.resource.org/cc/Notice> owl:equivalentClass cc:Notice .
<http://web.resource.org/cc/Permission> owl:equivalentClass cc:Permission .
<http://web.resource.org/cc/Prohibition> owl:equivalentClass cc:Prohibition .
<http://web.resource.org/cc/Reproduction> owl:equivalentClass cc:Reproduction .
<http://web.resource.org/cc/Requirement> owl:equivalentClass cc:Requirement .
<http://web.resource.org/cc/ShareAlike> owl:equivalentClass cc:ShareAlike .
<http://web.resource.org/cc/SourceCode> owl:equivalentClass cc:SourceCode .
<http://web.resource.org/cc/Work> owl:equivalentClass cc:Work .
<http://web.resource.org/cc/license> owl:equivalentProperty cc:license .
<http://web.resource.org/cc/permits> owl:equivalentProperty cc:permits .
<http://web.resource.org/cc/prohibits> owl:equivalentProperty cc:prohibits .
<http://web.resource.org/cc/requires> owl:equivalentProperty cc:requires .
cc:attributionName
  a rdf:Property ;
  rdfs:domain cc:Work ;
  rdfs:range rdfs:Literal .

cc:attributionURL
  a rdf:Property ;
  rdfs:domain cc:Work ;
  rdfs:range rdfs:Resource .

cc:deprecatedOn
  a rdf:Property ;
  rdfs:domain cc:License ;
  rdfs:label "deprecated on"@en-US ;
  rdfs:range <http://www.w3.org/2001/XMLSchema-datatypes#date> .

cc:jurisdiction
  a rdf:Property ;
  rdfs:domain cc:License ;
  rdfs:label "jurisdiction"@en-US ;
  rdfs:range cc:Jurisdiction .

cc:legalcode
  a rdf:Property ;
  rdfs:domain cc:License ;
  rdfs:range rdfs:Resource .

cc:license
  a rdf:Property ;
  owl:sameAs xhv:license ;
  rdfs:domain cc:Work ;
  rdfs:label "has license"@en-US ;
  rdfs:range cc:License ;
  rdfs:subPropertyOf dc:license .

cc:morePermissions
  a rdf:Property ;
  rdfs:domain cc:Work ;
  rdfs:range rdfs:Resource ;
  rdfs:subPropertyOf dc:relation .

cc:permits
  a rdf:Property ;
  rdfs:domain cc:License ;
  rdfs:label "permits"@en-US ;
  rdfs:range cc:Permission .

cc:prohibits
  a rdf:Property ;
  rdfs:domain cc:License ;
  rdfs:label "prohibits"@en-US ;
  rdfs:range cc:Prohibition .

cc:requires
  a rdf:Property ;
  rdfs:domain cc:License ;
  rdfs:label "requires"@en-US ;
  rdfs:range cc:Requirement .

cc:useGuidelines
  a rdf:Property ;
  rdfs:domain cc:Work ;
  rdfs:range rdfs:Resource ;
  rdfs:subPropertyOf dc:relation .

cc:Jurisdiction
  a rdfs:Class ;
  rdfs:comment "the legal jurisdiction of a license"@en-US ;
  rdfs:label "Jurisdiction"@en-US .

cc:License
  a rdfs:Class ;
  rdfs:comment "a set of requests/permissions to users of a Work, e.g. a copyright license, the public domain, information for distributors"@en-US ;
  rdfs:label "License"@en-US ;
  rdfs:subClassOf dc:LicenseDocument .

cc:Permission
  a rdfs:Class ;
  rdfs:comment "an action that may or may not be allowed or desired"@en-US ;
  rdfs:label "Permission"@en-US .

cc:Prohibition
  a rdfs:Class ;
  rdfs:comment "something you may be asked not to do"@en-US ;
  rdfs:label "Prohibition"@en-US .

cc:Requirement
  a rdfs:Class ;
  rdfs:comment "an action that may or may not be requested of you"@en-US ;
  rdfs:label "Requirement"@en-US .

cc:Work
  a rdfs:Class ;
  rdfs:comment "a potentially copyrightable work"@en-US ;
  rdfs:label "Work"@en-US .