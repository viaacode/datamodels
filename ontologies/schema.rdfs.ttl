@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf:<http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#####################################
#              CLASSES              #
#####################################

schema:ArchiveComponent a rdfs:Class;
    rdfs:subClassOf schema:CreativeWork ;
    rdfs:label "archive component"@en ;
    rdfs:label "archiefcomponent"@nl ;
    rdfs:label "composant d'archive"@fr ;
    skos:definition """An intangible type to be applied to any archive content, carrying with it a set of properties required to describe archival items and collections."""@en ;
    skos:definition """Een immaterieel type dat toegepast moet worden op elke inhoud van het archief, met daarbij een set van eigenschappen die nodig zijn om archiefitems en -collecties te beschrijven."""@nl ;
    skos:definition """Un type intangible à appliquer à tout contenu d'archives, portant avec lui un ensemble de propriétés nécessaires pour la description d'éléments d'archives et de collections."""@fr  ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:Collection
  a rdfs:Class ;
    rdfs:subClassOf schema:CreativeWork ;
    rdfs:label "collection"@en ;
    rdfs:label "collectie"@nl ;
    rdfs:label "collection"@fr ;
    skos:definition """A collection of items, e.g. creative works or products, typically but not necessarily of the same kind."""@en ;
    skos:definition """Een collectie van items, bv. creatieve werken (creative works) of producten, gewoonlijk maar niet altijd van dezelfde soort."""@nl ;
    skos:definition """Une collection d'articles, par exemple des œuvres créatives (creative works) ou des produits, généralement mais pas nécessairement de même type."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:ContactPoint a   rdfs:Class ;
  rdfs:subClassOf schema:StructuredValue ;
  rdfs:label "contactPoint"@en ;
  rdfs:label "point de contact"@fr ;
  rdfs:label "aanspreek-/meldpunt"@nl ;
  skos:definition "A contact point, for example a Customer Complaints department."@en ;
  skos:definition "Un point de contact, par exemple un département des plaintes clients."@fr ;
  skos:definition "Een aanspreekpunt, zoals een afdeling voor klachten van klanten."@nl ;
  rdfs:isDefinedBy <https://schema.org/> .

schema:Country a rdfs:Class ;
    rdfs:label "Country"@en ;
    rdfs:label "Pays"@fr ;
    rdfs:label "Land"@nl ;
    rdfs:comment "A country."@en ;
    rdfs:comment "Un pays."@fr ;
    rdfs:comment "Een land."@nl ;
    rdfs:subClassOf schema:AdministrativeArea ;
    rdfs:isDefinedBy <https://schema.org/> .


schema:CreativeWork a rdfs:Class;
    rdfs:subClassOf schema:Thing ;
    rdfs:label "creative work"@en ;
    rdfs:label "creatief werk"@nl ;
    rdfs:label "travail créatif"@fr ;
    skos:definition """The most generic kind of creative work, including books, movies, photographs, software programs, etc."""@en ;
    skos:definition """De meest generieke soort van creatief werk (CreativeWork), zoals boeken, films, foto's, programma's, enz."""@nl ;
    skos:definition """La plus générique des types de travail créatif (CreativeWork), y compris les livres, les films, les photos, les logiciels, etc."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:CreativeWorkSeason a rdfs:Class;
    rdfs:subClassOf schema:CreativeWork ;
    rdfs:label "creative work season"@en ;
    rdfs:label "creatief werk seizoen"@nl ;
    rdfs:label "Saison du travail créatif"@fr ;
    skos:definition """A media season e.g. tv, radio, video game etc."""@en ;
    skos:definition """Een mediaseizoen, bijvoorbeeld tv, radio, videogame etc."""@nl ;
    skos:definition """Une saison médiatique, par exemple tv, radio, jeu vidéo etc."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:CreativeWorkSeries a rdfs:Class;
    rdfs:subClassOf schema:CreativeWork ;
    rdfs:label "creative work series"@en ;
    rdfs:label "serie van het creatief werk"@nl ;
    rdfs:label "série du travail créatif"@fr ;
    skos:definition """A CreativeWorkSeries in schema.org is a group of related items, typically but not necessarily of the same kind. CreativeWorkSeries are usually organized into some order, often chronological. Unlike ItemList which is a general purpose data structure for lists of things, the emphasis with CreativeWorkSeries is on published materials (written e.g. books and periodicals, or media such as tv, radio and games)."""@en ;
    skos:definition """Een CreativeWorkSeries in schema.org is een groep vergelijkbare items, meestal maar niet altijd van dezelfde soort. CreativeWorkSeries zijn meestal gegroepeerd in een bepaalde volgorde, vaak chronologisch. In tegenstelling tot ItemList, ligt de nadruk van CreativeWorkSeries op gepubliceerd materiaal (bijvoorbeeld boeken en periodieken, of media zoals tv, radio en games)."""@nl ;
    skos:definition """Un CreativeWorkSeries dans schema.org est une série d'objets, typiquement mais pas forcément du même type. CreativeWorkSeries sont généralement organisées dans un certain ordre, souvent chronologique. Contrairement à ItemList, l'importance des CreativeWorkSeries est sur les matériaux publiés (par exemple des livres et des périodiques, ou des médias tels que tv, radio et jeux vidéo)."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:Episode a rdfs:Class;
    rdfs:subClassOf schema:CreativeWork ;
    rdfs:label "episode"@en ;
    rdfs:label "aflevering"@nl ;
    rdfs:label "épisode"@fr ;
    skos:definition """A media episode (e.g. TV, radio, video game) which can be part of a series or season."""@en ;
    skos:definition """Een media aflevering (bijvoorbeeld tv, radio, videogame) die deel uitmaakt van een serie of seizoen."""@nl ;
    skos:definition """Un épisode médiatique (par exemple tv, radio, jeu vidéo) qui peut faire partie d'une série ou d'une saison."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:Event a rdfs:Class ;
    rdfs:subClassOf schema:Thing ;
    rdfs:label "event"@en ;
    rdfs:label "gebeurtenis"@nl ;
    rdfs:label "événement"@fr ;
    skos:definition """An event happening at a certain time and location, such as a concert, lecture, or festival."""@en ;
    skos:definition """Een gebeurtenis die plaatsvindt op een gegeven moment en locatie, zoals een concert, een lezing of een festival."""@nl ;
    skos:definition """Un événement qui se déroule à un moment et dans un lieu donnés, comme un concert, une conférence ou un festival."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:Organization a rdfs:Class ;
    rdfs:subClassOf schema:Thing ;
    rdfs:label "organisation"@en ;
    rdfs:label "organisatie"@nl ;
    rdfs:label "organisation"@fr ;
    skos:definition """An organization such as a school, NGO, corporation, club, etc."""@en ;
    skos:definition """Een organisatie zoals een school, NGO, corporatie, club, enz."""@nl ;
    skos:definition """Une organisation telle que une école, une ONG, une société, un club, etc."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:PerformanceRole a rdfs:Class ;
    rdfs:subClassOf schema:Role ;
    rdfs:label "performance role"@en ;
    rdfs:label "uitvoeringsrol"@nl ;
    rdfs:label "rôle de performance"@fr ;
    skos:definition """A PerformanceRole is a Role that some entity places with regard to a theatrical performance, e.g. in a Movie, TVSeries etc."""@en ;
    skos:definition """Een PerformanceRole is een rol die een entiteit vertolkt met betrekking tot een theatrale uitvoering, bijvoorbeeld in een film, televisieserie etc."""@nl ;
    skos:definition """Un PerformanceRole est un rôle qu'une entité remplit dans le cadre d'une représentation théâtrale, par exemple dans un film, une série télévisée etc."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:Person   a   rdfs:Class ;
    rdfs:subClassOf schema:Thing ;
    owl:equivalentClass foaf:Person ;
    rdfs:label "person"@en ;
    rdfs:label "personne"@fr ;
    rdfs:label "persoon"@nl ;
    skos:definition "A person (alive, dead, undead, or fictional)."@en ;
    skos:definition "Une personne (vivante, morte, non-morte ou fictive)."@fr ;
    skos:definition "Een persoon (levend, dood, ondood, of fictief)."@nl ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:Place a rdfs:Class ;
    rdfs:subClassOf schema:Thing ;
    rdfs:label "place"@en ;
    rdfs:label "plaats"@nl ;
    rdfs:label "lieu"@fr ;
    skos:definition """Entities that have a somewhat fixed, physical extension."""@en ;
    skos:definition """Entiteiten die een enigszins vastliggende, fysieke uitbreiding hebben."""@nl ;
    skos:definition """Entités qui ont une extension physique quelque peu fixée."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:PostalAddress    a   rdfs:Class ;
  rdfs:subClassOf schema:ContactPoint ;
  rdfs:label "postal address"@en ;
  rdfs:label "adresse postale"@fr ;
  rdfs:label "postadres"@nl ;
  skos:definition "The mailing address."@en ;
  skos:definition "L'adresse postale."@fr ;
  skos:definition "Het postadres."@nl ;
  rdfs:isDefinedBy <https://schema.org/> .

schema:Country a rdfs:Class ;
    rdfs:label "country"@en ;
    rdfs:label "pays"@fr ;
    rdfs:label "land"@nl ;
    skos:definition "A country."@en ;
    skos:definition "Un pays."@fr ;
    skos:definition "Een land."@nl ;
    rdfs:subClassOf schema:AdministrativeArea .

schema:QuantitativeValue a rdfs:Class ;
    rdfs:subClassOf schema:Thing ;
    rdfs:label "quantitative Value"@en ;
    rdfs:label "kwantitatieve waarde"@nl ;
    rdfs:label "valeur quantitative"@fr ;
    skos:definition """A point value or interval for product characteristics and other purposes."""@en ;
    skos:definition """Een puntwaarde of interval voor producteigenschappen en andere doeleinden."""@nl ;
    skos:definition """Une valeur ponctuelle ou un intervalle pour les caractéristiques d'un produit et d'autres fins."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:Role a rdfs:Class ;
    rdfs:subClassOf schema:Thing ;
    rdfs:label "role"@en ;
    rdfs:label "rol"@nl ;
    rdfs:label "rôle"@fr ;
    skos:definition """Represents additional information about a relationship or property.
        For example a Role can be used to say that a 'member' role linking some SportsTeam to a player occurred during a particular time period.
        Or that a Person's 'actor' role in a Movie was for some particular characterName.
        Such properties can be attached to a Role entity, which is then associated with the main entities using ordinary properties like 'member' or 'actor'.
        """@en ;
    skos:definition """Stelt bijkomende informatie voor over een relatie of eigenschap.
        Bijvoorbeeld: een rol kan worden gebruikt om aan te geven dat een 'lid'rol (member role) die een sportteam aan een speler verbond, tijdens een bepaalde periode voorviel.
        Of dat een persoons 'acteur'rol (actor role) in een film voor een bepaalde karakternaam was.
        Deze eigenschappen kunnen worden toegevoegd aan een rolentiteit, die dan geassocieerd wordt met de hoofdentiteiten via normale eigenschappen zoals 'lid' (member) of 'acteur' (actor).
        """@nl ;
    skos:definition """Représente des informations supplémentaires sur une relation ou une propriété.
        Par exemple, un rôle peut être utilisé pour indiquer que une rôle 'membre' (member role) qui lie une équipe sportive à un joueur, a eu lieu durant une période particulière.
        Ou que le rôle d''acteur' (actor role) d'une personne dans un film correspond à un nom de personnage particulier.
        Ces propriétés peuvent être attachées à un rôle d'entité (entity role), qui est ensuite associée aux entités principales en utilisant des propriétés ordinaires comme 'membre' (member) ou 'acteur' (actor).
        """@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

#####################################
#             PROPERTIES            #
#####################################

schema:about a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range  schema:Thing ;  
    rdfs:label "about"@en ;
    rdfs:label "over"@nl ;
    rdfs:label "sur"@fr ;
    skos:definition """The subject matter of the content."""@en ;
    skos:definition """Het onderwerp van de inhoud."""@nl ;
    skos:definition """Le sujet du contenu."""@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:abstract a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range xsd:string ;
    owl:equivalentProperty dct:abstract ; 
    rdfs:label "abstract"@en ;
    rdfs:label "abstract"@nl ;
    rdfs:label "résumé"@fr ;
    skos:definition "An abstract is a short description that summarizes a CreativeWork."@en ;
    skos:definition "Een abstract is een korte beschrijving die een CreativeWork samenvat."@nl ;
    skos:definition "Un résumé est une description courte qui résume un CreativeWork."@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:actor a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range schema:Person ;
    rdfs:label "actor"@en ;
    rdfs:label "acteur"@nl ;
    rdfs:label "acteur"@fr ;
    skos:definition "An actor, e.g. in tv, radio, movie, video games etc., or in an event. Actors can be associated with individual items or with a series, episode, clip."@en ;
    skos:definition "Een acteur, bv. in tv, radio, film, videogames etc. of in een evenement. Acteurs kunnen geassocieerd worden met met individuele items of met een serie, aflevering, clip."@nl ;
    skos:definition "Un acteur, par exemple à la télévision, à la radio, au cinéma, dans les jeux vidéo, etc. ou dans un événement. Les acteurs peuvent être associés à des éléments individuels ou à une série, un épisode, un clip."@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:addressCountry   a   rdf:Property ;
    schema:domainIncludes schema:DefinedRegion,
        schema:GeoCoordinates,
        schema:GeoShape,
        schema:PostalAddress ;
    schema:rangeIncludes schema:Country,
        schema:Text ;
    rdfs:label "addressCountry"@en ;
    rdfs:label "partie de l'adresse concernant le pays"@fr ;
    rdfs:label "deel van het adres met betrekking tot het land"@nl ;
    skos:definition "The country. For example, USA. You can also provide the two-letter [ISO 3166-1 alpha-2 country code](http://en.wikipedia.org/wiki/ISO_3166-1)."@en ;
    skos:definition "Le pays."@fr ;
    skos:definition "Het land."@nl ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:addressLocality  a   rdf:Property ;
    schema:domainIncludes schema:PostalAddress ;
    schema:rangeIncludes schema:Text ;
    rdfs:label "addressLocality"@en ;
    rdfs:label "localitéAdresse"@fr ;
    rdfs:label "adresPlaats"@nl ;
    skos:definition "The locality in which the street address is, and which is in the region. For example, Mountain View."@en ;
    skos:definition "La localité dans laquelle se trouve l'adresse de rue, et qui se trouve dans la région."@fr ;
    skos:definition "De plaats waar het straatadres zich bevindt, en de plaats van de regio."@nl ;
    rdfs:isDefinedBy <https://schema.org/> .
    
schema:addressRegion    a   rdf:Property ;
    schema:domainIncludes schema:DefinedRegion,
        schema:PostalAddress ;
    schema:rangeIncludes schema:Text ;
    rdfs:label "addressRegion"@en ;
    rdfs:label "addresseRegion"@fr ;
    rdfs:label "adresRegio"@nl ;
    skos:definition "The region in which the locality is, and which is in the country. For example, California or another appropriate first-level [Administrative division](https://en.wikipedia.org/wiki/List_of_administrative_divisions_by_country)."@en ;
    skos:definition "La région dans laquelle se trouve la localité, et qui se trouve dans le pays. Par exemple, la Californie ou [une autre division administrative appropriée de premier niveau](https://fr.wikipedia.org/wiki/Liste_des_subdivisions_par_pays)."@fr ;
    skos:definition "De regio waarin de plaats zich bevindt, en die in het land ligt. Bijvoorbeeld, Californië of een ander geschikte [administratieve afdeling van het eerste niveau](https://nl.wikipedia.org/wiki/Bestuurslagen_per_land)."@nl ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:alternateName a rdf:Property ;
    rdfs:domain schema:Thing ;
    rdfs:range xsd:string ;
    owl:equivalentProperty dct:alternative ;
    rdfs:label "alternate name"@en ;
    rdfs:label "alternatieve naam"@nl ;
    rdfs:label "nom alternatif"@fr ;
    skos:definition "An alias for the item"@en ;
    skos:definition "Een alias voor het item"@nl ;
    skos:definition "Un alias pour l'élément"@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:artform a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range xsd:string ;
    rdfs:label "artform"@en ;
    rdfs:label "kunstvorm"@nl ;
    rdfs:label "forme artistique"@fr ;
    skos:definition "The artform, medium, or mode of this creative work for example Painting, Drawing, Sculpture, Print, Photograph, Assemblage, Collage, etc."@en ;
    skos:definition "De kunstvorm, het medium of de vorm van dit creatieve werk, bijvoorbeeld Schilderij, Tekening, Beeldhouwwerk, Print, Foto, Assemblage, Collage, etc."@nl ;
    skos:definition "La forme artistique, le support ou le mode de ce travail créatif, par exemple Peinture, Dessin, Sculpture, Impression, Photographie, Assemblage, Collage, etc."@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:artMedium a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range xsd:string ;
    owl:equivalentProperty dct:medium ;
    rdfs:label "art medium"@en ;
    rdfs:label "kunst medium"@nl ;
    rdfs:label "médium artistique"@fr ;
    skos:definition "The material used. (e.g. Oil, Watercolour, Acrylic, Linoprint, Marble, Cyanotype, Digital, Lithograph, DryPoint, Intaglio, Pastel, Woodcut, Pencil, Mixed Media, etc.)"@en ;
    skos:definition "Het gebruikte materiaal. (bv. Olie, Aquarel, Acryl, Linoprint, Marmer, Cyanotype, Digitaal, Lithografie, Droogpunt, Intaglio, Pastel, Houtsnede, Potlood, Gemengde media, etc.)"@nl ;
    skos:definition "Le matériau utilisé. (par exemple Huile, Aquarelle, Acrylique, Linoprint, Marbre, Cyanotype, Numérique, Lithographie, Point sec, Intaglio, Pastel, Gravure sur bois, Crayon, Médias mixtes, etc.)"@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:author a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ owl:unionOf(schema:Person org:Organization schema:Role) ] ;
    owl:equivalentProperty dct:creator ;
    rdfs:label "author"@en ;
    rdfs:label "auteur"@nl ;
    rdfs:label "auteur"@fr ;
    skos:definition "The author of this content or rating."@en ;
    skos:definition "De auteur van deze inhoud of beoordeling."@nl ;
    skos:definition "L'auteur de ce contenu ou de cette évaluation."@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:birthDate a rdf:Property ;
    rdfs:domain schema:Person ;
    rdfs:range xsd:date ; 
    rdfs:label "birth date"@en ;
    rdfs:label "date de naissance"@fr ;
    rdfs:label "geboortedatum"@nl ;
    skos:definition "Date of birth"@en ;
    skos:definition "Date de naissance"@fr ;
    skos:definition "Geboortedatum"@nl ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:caption a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range xsd:string ;
    rdfs:label "caption"@en ;
    rdfs:label "ondertiteling"@nl ;
    rdfs:label "sous-titrage"@fr ;
    skos:definition "The caption for this object."@en ;
    skos:definition "De ondertiteling voor dit object."@nl ;
    skos:definition "Les sous-titres de cet objet."@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

#TODO SHACL domein organization range contact point toevoegen
schema:contactPoint a   rdf:Property ;
    schema:domainIncludes schema:HealthInsurancePlan,
        schema:Organization,
        schema:Person ;
    schema:rangeIncludes schema:ContactPoint ;
    rdfs:label "contactPoint"@en ;
    rdfs:label "pointDeContact"@fr ;
    rdfs:label "aanspreekpunt"@nl ;
    # labels van verwijderd haOrg:hasContactPoint
    # rdfs:label "has a contact point"@en ;
    # rdfs:label "a un point de contact"@fr ;
    # rdfs:label "heeft een aanspreek-/meldpunt"@nl ; 
    skos:definition "A contact point for a person or organization."@en ;
    skos:definition "Un point de contact pour une personne ou une organisation."@fr ;
    skos:definition "Een aanspreekpunt voor een persoon of een organisatie."@nl ;
    rdfs:isDefinedBy <https://schema.org/> .
    
schema:contactType  a   rdf:Property ;
    schema:domainIncludes schema:ContactPoint ;
    schema:rangeIncludes schema:Text ;
    rdfs:label "contactType"@en ;
    rdfs:label "typePointContact"@fr ;
    rdfs:label "contactType"@nl ;
    skos:definition "A person or organization can have different contact points, for different purposes. For example, a sales contact point, a PR contact point and so on. This property is used to specify the kind of contact point."@en ;
    skos:definition "Une personne ou une organisation peut avoir différents points de contact, à des fins différentes. Par exemple, un point de contact pour les ventes, un point de contact pour les relations publiques, etc. Cette propriété est utilisée pour spécifier le type de point de contact."@fr ;
    skos:definition "Een persoon of organisatie kan verschillende contactpunten hebben, voor verschillende doeleinden. Bijvoorbeeld een verkoopcontactpunt, een PR-contactpunt enzovoort. Deze eigenschap wordt gebruikt om het soort contactpunt te specificeren."@nl ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:contributor a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ owl:unionOf(schema:Person org:Organization schema:Role) ] ;
    owl:equivalentProperty dct:contributor ;
    rdfs:label "contributor"@en ;
    rdfs:label "bijdrager"@nl ;
    rdfs:label "contributeur"@fr ;
    skos:definition "A secondary contributor to the CreativeWork or Event."@en ;
    skos:definition "Een tweede bijdrager aan het CreativeWork of Event."@nl ;
    skos:definition "Un contributeur secondaire à la CreativeWork ou à le Event."@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:copyrightHolder a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ owl:unionOf(schema:Person org:Organization schema:Role) ] ;
    owl:equivalentProperty dct:rightsHolder ;
    rdfs:label "copyright holder"@en ;
    rdfs:label "auteursrechthebbende"@nl ;
    rdfs:label "titulaire du droit d'auteur"@fr ;
    skos:definition "The party holding the legal copyright to the CreativeWork."@en ;
    skos:definition "De partij die de auteursrechten heeft op het CreativeWork."@nl ;
    skos:definition "La partie qui détient le droit d'auteur légal de l'œuvre créative (CreativeWork)."@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:copyrightNotice a rdf:Property ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range xsd:string ;
    owl:equivalentProperty dct:rights ;
    rdfs:label "copyright notice"@en ;
    rdfs:label "auteursrechtvermelding"@nl ;
    rdfs:label "avis de droit d'auteur"@fr ;
    skos:definition "Text of a notice appropriate for describing the copyright aspects of this Creative Work, ideally indicating the owner of the copyright for the Work."@en ;
    skos:definition "Tekst van een mededeling die geschikt is om de auteursrechtelijke aspecten van dit CreativeWork te beschrijven, idealiter met vermelding van de auteursrechthebbende van het werk."@nl ;
    skos:definition "Texte d'une notice appropriée pour décrire les aspects du droit d'auteur de cette CreativeWork, indiquant idéalement le propriétaire du droit d'auteur de l'oeuvre."@fr ;
    rdfs:isDefinedBy <https://schema.org/> .

schema:copyrightYear a rdf:Property ;
