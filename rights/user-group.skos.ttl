    skos:inScheme <https://data.hetarchief.be/id/user-group> ;
    skos:prefLabel "public"@en,
        "Publiek"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/user-group> .

<https://data.hetarchief.be/id/user-group/research-public> a skos:Concept ;
    skos:definition "Anyone who falls under the Scientific Research user group as described in Appendix 2 of the meemoo collaboration agreement."@en,
