@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.hetarchief.be/id/metadata-range/extended> a skos:Concept,
        <https://data.hetarchief.be/ns/rights/MetadataRange> ;
    skos:definition "The metadata will be displayed in the form of limited set, according to the agreements."@en,
        "Een uitgebreide set metadata, bepaald volgens afspraken en documentatie, wordt ontsloten."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/metadata-range> ;
    skos:prefLabel "Extended"@en,
        "Étendu"@fr,
        "Uitgebreid"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/metadata-range> .

<https://data.hetarchief.be/id/metadata-range/limited> a skos:Concept,
        <https://data.hetarchief.be/ns/rights/MetadataRange> ;
    skos:definition "The metadata will be displayed in the form of a limited set, according to the agreements."@en,
        "Een beperkte set metadata, bepaald volgens afspraken en documentatie, wordt ontsloten."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/metadata-range> ;
    skos:prefLabel "Limited"@en,
        "Limité"@fr,
        "Beperkt"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/metadata-range> .

<https://data.hetarchief.be/id/metadata-range> a skos:ConceptScheme ;
    skos:hasTopConcept <https://data.hetarchief.be/id/metadata-range/extended>,
        <https://data.hetarchief.be/id/metadata-range/limited> ;
    skos:prefLabel "Thesaurus metadatabereik"@nl .

