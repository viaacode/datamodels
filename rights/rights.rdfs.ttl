@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix haRig: <https://data.hetarchief.be/ns/rights/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

haRig:Motivation a rdfs:Class;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Motivatie"@nl ;
    rdfs:label "Motivation"@en ;
    rdfs:label "Motivation"@fr ;
    skos:definition "Motivatie voor een ontsluitingsbeperking."@nl ;
    skos:definition "Motivation for an access limitation."@en ;
    skos:definition "Motivation pour une limitation d'accès."@fr ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/rights/> .

# right operands classes

haRig:UserGroup a rdfs:Class;
    rdfs:subClassOf odrl:RightOperand ;
    rdfs:label "Gebruikersgroep"@nl ;
    rdfs:label "User group"@en ;
    rdfs:label "Groupe d'utilisateurs"@fr ;
    skos:definition "Groep van web platform gebruikers."@nl ;
    skos:definition "Group of web platform users."@en ;
    skos:definition "Groupe d'utilisateurs de la plate-forme Web."@fr ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/rights/> .

haRig:MetadataRange a rdf:Class;
    rdfs:subClassOf odrl:RightOperand ;
    rdfs:label "Metadatabereik"@nl ;
    rdfs:label "Metadata range"@en ;
    rdfs:label "Gamme de métadonnées"@fr ;
    skos:definition "Het beoogde bereik van de metadata van het object. Bijvoorbeeld, of het over de volledige set aan metadata gaat of slechts een deel."@nl ;
    skos:definition "The intended reach of the object's metadata. For example, whether it is about the full set of metadata or only a part."@en ;
    skos:definition "La portée prévue des métadonnées de l'objet.Par exemple, qu'il s'agisse de l'ensemble complet de métadonnées ou seulement une pièce."@fr ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/rights/> .

haRig:ContentRange a rdfs:Class;
    rdfs:subClassOf odrl:RightOperand ;
    rdfs:label "Inhoudsbereik"@nl ;
    rdfs:label "Content range"@en ;
    rdfs:label "Plage de contenu"@fr ;
    skos:definition "Het beoogde bereik van de mediainhoud van het object. Bijvoorbeeld, of het over de volledige inhoud gaat of slechts een deel."@nl ;
    skos:definition "The intended reach of the object's media content of the. For example, whether it is about the full content or only a part."@en ;
    skos:definition "La portée prévue du contenu multimédia de l'objet du.Par exemple, qu'il s'agisse du contenu complet ou seulement une pièce."@fr ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/rights/> .

haRig:isMotivatedBy a rdf:Property;
    rdfs:domain odrl:Prohibition;
    rdfs:range haRig:Motivation;
    rdfs:label  "is motivated by"@en ;
    rdfs:label  "est motivé par"@fr ;
    rdfs:label  "wordt gemotiveerd door"@nl ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/rights/> .

# left operands (inviduals)

haRig:metadataRange a odrl:LeftOperand, owl:NamedIndividual, skos:Concept ;
    skos:prefLabel "Metadatabereik"@nl ;
    rdfs:label "Metadatabereik"@nl ;
    skos:prefLabel "Metadata range"@en ;
    rdfs:label "Metadata range"@en ;
    skos:prefLabel "Gamme de métadonnées"@fr ;
    rdfs:label "Gamme de métadonnées"@fr ;
    skos:definition "Het beoogde bereik van de metadata van het object. Bijvoorbeeld, of het over de volledige set aan metadata gaat of slechts een deel."@nl ;
    skos:definition "The intended reach of the object's metadata. For example, whether it is about the full set of metadata or only a part."@en ;
    skos:definition "La portée prévue des métadonnées de l'objet.Par exemple, qu'il s'agisse de l'ensemble complet de métadonnées ou seulement une pièce."@fr ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/rights/> .

haRig:contentRange a odrl:LeftOperand, owl:NamedIndividual, skos:Concept ;
    skos:prefLabel "Inhoudsbereik"@nl ;
    rdfs:label "Inhoudsbereik"@nl ;
    skos:prefLabel "Content range"@en ;
    rdfs:label "Content range"@en ;
    skos:prefLabel "Plage de contenu"@fr ;
    rdfs:label "Plage de contenu"@fr ;
    skos:definition "Het beoogde bereik van de mediainhoud van het object. Bijvoorbeeld, of het over de volledige inhoud gaat of slechts een deel."@nl ;
    skos:definition "The intended reach of the object's media content of the. For example, whether it is about the full content or only a part."@en ;
    skos:definition "La portée prévue du contenu multimédia de l'objet du.Par exemple, qu'il s'agisse du contenu complet ou seulement une pièce."@fr ;
    rdfs:isDefinedBy <https://data.hetarchief.be/ns/rights/> .

# right operands - individuals

haRig:available-for-consultation a skos:Concept, odrl:Action, owl:NamedIndividual ;
    skos:altLabel "listen"@en,
        "view"@en,
        "bekijken"@nl,
        "beluisteren"@nl ;
    skos:definition "To make a digital copy available for consultation by third-parties."@en,
        "Om een ​​digitale kopie beschikbaar te stellen voor raadpleging door derden."@nl ;
    skos:exactmatch odrl:display ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Make available for consultation"@en,
        "Raadpleegbaar maken"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:downloadable a skos:Concept, odrl:Action, owl:NamedIndividual ;
    skos:altLabel "download"@en,
        "kopie downloaden"@nl ;
    skos:definition "To make the digital copy downloadable by third-parties."@en,
        "Om de digitale kopie downloadbaar te maken voor derden."@nl ;
    skos:exactmatch odrl:distribute ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Make downloadable"@en,
        "Downloadbaar maken"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:full a skos:Concept, haRig:ContentRange, owl:NamedIndividual ;
    skos:definition "The material can be displayed in full, according to the agreements made."@en,
        "De toestemming is van toepassing op het materiaal in zijn geheel."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Full"@en,
        "Complet"@fr,
        "Volledig"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:partial a skos:Concept, haRig:ContentRange, owl:NamedIndividual ;
    skos:definition "The material can be displayed in part, according to the agreements made."@en,
        "De toestemming is van toepassing op het materiaal in de vorm van één of meerdere fragmenten, volgens een aangegeven tijdsstempel of volgens de bilaterale afspraken (zodanig dat de duur van het fragment minder dan de helft van de totale duur van het materiaal is)."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Partial"@en,
        "Partiel"@fr,
        "Deels"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:extended a skos:Concept, haRig:MetadataRange, owl:NamedIndividual ;
    skos:definition "The metadata will be displayed in the form of limited set, according to the agreements."@en,
        "Een uitgebreide set metadata, bepaald volgens afspraken en documentatie, wordt ontsloten."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Extended"@en,
        "Étendu"@fr,
        "Uitgebreid"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:limited a skos:Concept, haRig:MetadataRange, owl:NamedIndividual ;
    skos:definition "The metadata will be displayed in the form of a limited set, according to the agreements."@en,
        "Een beperkte set metadata, bepaald volgens afspraken en documentatie, wordt ontsloten."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Limited"@en,
        "Limité"@fr,
        "Beperkt"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:between-partners a skos:Concept, haRig:UserGroup, owl:NamedIndividual ;
    skos:definition "Anyone who has an collaboration agreement with meemoo that includes key use as permission (meemoo collaboration agreement 6.7.3 or 5.7.3)"@en,
        "Iedereen die een samenwerkingsovereenkomst met meemoo heeft waarin sleutelgebruik als toestemming is opgenomen (meemoo samenwerkingsovereenkomst 6.7.3 of 5.7.3)"@nl ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Between partners"@en,
        "Tussen partners"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:educational-public a skos:Concept, haRig:UserGroup, owl:NamedIndividual ;
    skos:definition "Anyone subject to the conditions for access to the Archive for Education platform (e.g., lecturers, learners, educational support staff, etc.), see Appendix 2 of the meemoo collaboration agreement."@en,
        "Iedereen die valt onder de voorwaarden van toegang tot het Archief voor Onderwijsplatform (bv lesgevers, lerenden, onderwijsondersteunend personeel...), zie bijlage 2 van de meemoo samenwerkingsovereenkomst."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Educational public"@en,
        "Onderwijs"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:intra-muros a skos:Concept, haRig:UserGroup, owl:NamedIndividual ;
    skos:definition "Anyone who is within the institution's buildings for the purpose of digital consultation."@en,
        "Iedereen die zich binnen de gebouwen van de instelling bevindt met het oog op digitale raadpleging."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Intra muros"@en,
        "Intra muros"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:public a skos:Concept, haRig:UserGroup, owl:NamedIndividual ;
    skos:definition "Anyone with internet access."@en,
        "Iedereen met toegang tot internet."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "public"@en,
        "Publiek"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .

haRig:research-public a skos:Concept, haRig:UserGroup, owl:NamedIndividual ;
    skos:definition "Anyone who falls under the Scientific Research user group as described in Appendix 2 of the meemoo collaboration agreement."@en,
        "Iedereen die valt onder de gebruikersgroep Wetenschappelijk Onderzoek zoals beschreven in bijlage 2 van de meemoo samenwerkingsovereenkomst."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/constraint-operand> ;
    skos:prefLabel "Research public"@en,
        "Onderzoek"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/constraint-operand> .


<https://data.hetarchief.be/id/constraint-operand> a skos:ConceptScheme ;
    skos:prefLabel "Thesaurus rechteroperanden"@nl, "Thesaurus rightoperands"@nl ;
    skos:hasTopConcept haRig:extended,
        haRig:limited,
        haRig:full,
        haRig:partial,
        haRig:available-for-consultation,
        haRig:downloadable ,
        haRig:between-partners,
        haRig:educational-public,
        haRig:intra-muros,
        haRig:public,
        haRig:research-public .

<https://data.hetarchief.be/id/collection/action> a skos:Collection ;
    skos:member haRig:available-for-consultation,
        haRig:downloadable ;
    skos:prefLabel "Ontsluitingsacties"@nl .

<https://data.hetarchief.be/id/collection/user-group> a skos:Collection ;
    skos:member haRig:between-partners,
        haRig:educational-public,
        haRig:intra-muros,
        haRig:public,
        haRig:research-public ;
    skos:prefLabel "Gebruikersgroepen"@nl .

<https://data.hetarchief.be/id/collection/metadata-range> a skos:Collection ;
    skos:member haRig:extended,
        haRig:limited ;
    skos:prefLabel "Metadatabereik"@nl .

<https://data.hetarchief.be/id/collection/content-range> a skos:Collection ;
    skos:member haRig:full,
        haRig:partial ;
    skos:prefLabel "Inhoudsbereik"@nl .