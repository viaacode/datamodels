@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.hetarchief.be/id/action/available-for-consultation> a skos:Concept,
        odrl:Action ;
    skos:altLabel "listen"@en,
        "view"@en,
        "bekijken"@nl,
        "beluisteren"@nl ;
    skos:definition "To make a digital copy available for consultation by third-parties."@en,
        "Om een ​​digitale kopie beschikbaar te stellen voor raadpleging door derden."@nl ;
    skos:exactmatch odrl:display ;
    skos:inScheme <https://data.hetarchief.be/id/action> ;
    skos:prefLabel "Make available for consultation"@en,
        "Raadpleegbaar maken"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/action> .

<https://data.hetarchief.be/id/action/downloadable> a skos:Concept,
        odrl:Action ;
    skos:altLabel "download"@en,
        "kopie downloaden"@nl ;
    skos:definition "To make the digital copy downloadable by third-parties."@en,
        "Om de digitale kopie downloadbaar te maken voor derden."@nl ;
    skos:exactmatch odrl:distribute ;
    skos:inScheme <https://data.hetarchief.be/id/action> ;
    skos:prefLabel "Make downloadable"@en,
        "Downloadbaar maken"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/action> .

<https://data.hetarchief.be/id/action> a skos:ConceptScheme ;
    skos:hasTopConcept <https://data.hetarchief.be/id/action/available-for-consultation>,
        <https://data.hetarchief.be/id/action/downloadable> ;
    skos:prefLabel "Thesaurus ontsluitingsacties"@nl .

