<https://data.hetarchief.be/id/permission/intramuros-materiaal-volledig-raadplegen> a skos:Concept, odrl:Permission ;
    skos:definition "The full content may be viewed/listened to within the community."@en,
        "Het volledige materiaal mag intra muros bekeken/beluisterd worden."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/permission> ;
    skos:prefLabel "intra muros - content - full - consult"@en,
        "intramuros - materiaal - volledig - raadplegen"@nl ;
    skos:topConceptOf <https://data.hetarchief.be/id/permission> ;
     #  welke actie: raadplegen
    odrl:action haRig:available-for-consultation;
    #  aan wie?: intra muros
    odrl:constraint [
        odrl:leftOperand odrl:recipient;
        odrl:operator odrl:eq;
        odrl:rightOperand haRig:intra-muros
    ];
    #  welk materiaal?: volledig
    odrl:constraint [
        odrl:leftOperand haRig:contentRange;
        odrl:operator odrl:eq;
        odrl:rightOperand haRig:full
    ].

