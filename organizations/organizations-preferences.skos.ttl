@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

@base <https://data.hetarchief.be/id/organization-preferences/> .

<preference>  a skos:Concept ;
    skos:narrower <visitor-space-publication>, <iiif-publication>, <logo-embedding> ,
                  <the-archive-publication>, <the-archive-for-education-permission> ;
    skos:prefLabel  "Preference"@en, 
                    "Préférence"@fr, 
                    "Voorkeur"@nl ;
    skos:definition "An organization's preference regarding whether or not to perform a particular service in collaboration with/by meemoo."@en, 
                    "Préférence d'une organisation quant à l'exécution ou non d'un certain service en collaboration avec/par meemoo."@fr, 
                    "Voorkeur van een organisatie rond het al dan niet uitvoeren van een bepaalde dienst in samenwerking met/door meemoo."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/organization-preferences> .

<the-archive-visitor-space-publication>  a skos:Concept ;
    skos:broader <preference> ;
    skos:prefLabel  "Publication on the digital visitor space on thearchive.be"@en, 
                    "Publication sur l'espace des visiteurs digitale de hetarchief.be"@fr,
                    "Publicatie op de digitale bezoekersruimte van hetarchief.be"@nl ;
    skos:definition "Indication of whether the organization wants its own digital objects to be published on the meemoo Visitor Space on thearchive.be."@en, 
                    "Indication si l'organisation souhaite que ses propres objets numériques soient publiés sur l'espace des visiteurs de meemoo sur hetarchief.be."@fr,
                    "Aanduiding of de organisatie wenst dat de eigen digitale objecten gepubliceerd worden op de Bezoekertool van meemoo op hetarchief.be."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/organization-preferences> .

<the-archive-publication> a skos:Concept ;
    skos:broader <preference> ;
    skos:prefLabel  "Publication on thearchive.be"@en, 
                    "Publication sur hetarchief.be"@fr, 
                    "Publicatie op hetarchief.be"@nl ;
    skos:definition "Indication of whether the organization wants its own digital objects to be published on The Archive."@en, 
                    "Indication si l'organisation souhaite que ses propres objets numériques soient publiés sur Het Archief."@fr,
                    "Aanduiding of de organisatie wenst dat de eigen digitale objecten gepubliceerd worden op Het Archief."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/organization-preferences> .

<the-archive-for-education-publication> a skos:Concept ;
    skos:broader <preference> ;
    skos:prefLabel  "Publication on onderwijs.hetarchief.be"@en, 
                    "Publication sur onderwijs.hetarchief.be"@fr, 
                    "Publicatie op onderwijs.hetarchief.be"@nl ;
    skos:definition "Indication of whether the organization wants to give permission to use its own digital objects for educational purposes on onderwijs.hetarchief.be."@en,
                    "Indication si l'organisation souhaite donner l'autorisation d'utiliser ses propres objets numériques à des fins éducatives sur onderwijs.hetarchief.be."@fr,
                    "Aanduiding of de organisatie toestemming wenst te geven om de eigen digitale objecten te gebruiken voor educatieve doeleinden op onderwijs.hetarchief.be."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/organization-preferences> .

<iiif-publication>  a skos:Concept ;
    skos:broader <preference> ;
    skos:prefLabel  "Publication via IIIF"@en,
                    "Publication via IIIF"@fr,
                    "Publicatie via IIIF"@nl ;
    skos:definition "Indication of whether or not the organization wants its own digital objects to be made available via meemoo's IIIF service."@en,
                    "Indication si l'organisation souhaite ou non que ses propres objets numériques soient accessibles via le service IIIF de meemoo."@fr,
                    "Aanduiding of de organisatie al dan niet wenst dat de eigen digitale objecten ontsloten kunnen worden via meemoo's IIIF service."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/organization-preferences> .

<logo-embedding>  a skos:Concept ;
    skos:broader <preference> ;
    skos:prefLabel  "Logo embedding"@en, 
                    "Intégration du logo"@fr,
                    "Inbedding van het logo"@nl ;
    skos:definition "Indication of whether or not the organization wants its own logo to be embedded in its own digital objects (via a (digital) watermark) during display on meemoo's platforms."@en,
                    "Indication si l'organisation souhaite ou non que son propre logo soit intégré dans ses propres objets numériques (via un filigrane (numérique)) lors de l'affichage sur les plateformes de meemoo."@fr,
                    "Aanduiding of de organisatie al dan niet wenst dat het eigen logo wordt ingebed in haar eigen digitale objecten (via een (digitaal) watermerk) tijdens weergave op de platformen van meemoo."@nl ;
    skos:inScheme <https://data.hetarchief.be/id/organization-preferences> .
